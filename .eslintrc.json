{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "rules": {
        "no-console": 0,
        "comma-dangle": ["error", "always"],
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es6": true,
        "jasmine": true,
    },
    "plugins": [
        "babel",
        "react"
    ]
}
